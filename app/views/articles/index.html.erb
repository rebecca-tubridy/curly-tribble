<h1 class="page-header">Articles</h1>
<div class="container">
  <div class="row">
    <ol>
      <% @articles.each do |article| %>
        <li class="article-card">
          <%= link_to article.title, article %>
        </li>
      <% end %>
    </ol>
  </div>
</div>


<button class="btn" style="margin-top: 60px;">
<%= link_to "New Article", new_article_path %>
</button>


