<hr>
<h2 class="page-header">꧁ Login ꧂</h2> 
<hr>  
<%= form_tag sessions_path do %>   
<div class="form">
  <div>
    <%= label_tag :email %><br>   
    <%= text_field_tag :email %>
  </div>
  <div>
    <%= label_tag :password %><br>   
    <%= password_field_tag :password %>   
  </div>
  <%= submit_tag "Log In" %>
</div> 
<% end %>
